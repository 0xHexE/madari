{
  "name": "@madari/web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "extract": "formatjs extract extract './src/**/*.{ts,tsx}' --ignore='**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile",
    "compile.mr": "yarn compile lang/mr.json --ast --out-file compiled-lang/mr.json",
    "compile.en": "yarn compile lang/en.json --ast --out-file compiled-lang/en.json",
    "compile:all": "yarn compile.en && yarn compile.mr"
  },
  "dependencies": {
    "@vidstack/react": "^1.12.12",
    "baseui": "0.0.0-next-18f01efe0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.454.0",
    "maverick.js": "^0.43.2",
    "pako": "^2.1.0",
    "puppeteer": "10.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.13.1",
    "react-intl": "^6.8.4",
    "react-router-dom": "^6.27.0",
    "react-window": "^1.8.10",
    "react-window-infinite-loader": "^1.0.9",
    "styletron-engine-atomic": "^1.6.2",
    "styletron-engine-monolithic": "^1.0.0",
    "styletron-react": "^6.1.1",
    "vidstack": "^0.6.15"
  },
  "prettier": {
    "singleQuote": true,
    "semi": true
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@formatjs/cli": "^6.3.5",
    "@types/lodash": "^4",
    "@types/node": "^22.8.5",
    "@types/pako": "^2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-window": "^1",
    "@types/react-window-infinite-loader": "^1",
    "@vitejs/plugin-react": "^4.3.3",
    "babel-plugin-formatjs": "^10.5.22",
    "eslint": "^9.13.0",
    "eslint-plugin-baseui": "^13.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
